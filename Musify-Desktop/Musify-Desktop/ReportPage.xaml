<Page
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Musify_Desktop"
        xmlns:MusiFy_Library="clr-namespace:MusiFy_Library;assembly=MusiFy-Library" x:Class="Musify_Desktop.ReportPage"
        mc:Ignorable="d"
        Background="#091227"
        Title="ReportPage" Height="450" Width="800">

    <Viewbox>
        <Grid Width="800" Height="450">
            <Label Content="MusiFy" HorizontalAlignment="Center" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="#FFD8D8D8" FontSize="40" FontFamily="Segoe UI" FontWeight="Bold"/>
            <Grid  Height="300" Width="750" Margin="0,60,0,0">
                <StackPanel Margin="60,0 ,0 ,0" HorizontalAlignment="Left" Width="300" x:Name="reportStack" Orientation="Vertical"></StackPanel>
                <StackPanel Margin="0,0 ,60 ,0"  HorizontalAlignment="Right" Width="300" x:Name="reportStackTwo" Orientation="Vertical" ></StackPanel>
            </Grid>
        </Grid>
    </Viewbox>
</Page>
